{
    "name": "infinityx-admin",
    "displayName": "InfinityX Admin",
    "description": "MCP-governed intelligence cockpit and Auto Builder controls",
    "version": "0.1.0",
    "publisher": "infinityxone",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "infinityx.open.visionCortex",
                "title": "InfinityX: Open Vision Cortex"
            },
            {
                "command": "infinityx.open.autoBuilder",
                "title": "InfinityX: Open Auto Builder"
            },
            {
                "command": "infinityx.open.debateArena",
                "title": "InfinityX: Open Debate Arena"
            }
        ],
        "configuration": {
            "title": "InfinityX Admin",
            "properties": {
                "infinityx.mcpEndpoint": {
                    "type": "string",
                    "default": "http://localhost:8000",
                    "description": "MCP Orchestrator endpoint URL"
                },
                "infinityx.defaultMode": {
                    "type": "string",
                    "enum": [
                        "DRY_RUN",
                        "VALIDATED",
                        "LIVE"
                    ],
                    "default": "DRY_RUN",
                    "description": "Default execution mode for Auto Builder"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "lint": "eslint src --ext ts"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/vscode": "^1.85.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.0.0",
        "typescript": "^5.3.0"
    },
    "dependencies": {
        "@react-three/drei": "^10.7.7",
        "@react-three/fiber": "^9.4.2",
        "autoprefixer": "^10.4.23",
        "firebase": "^12.7.0",
        "framer-motion": "^12.23.26",
        "luxon": "^3.7.2",
        "node-fetch": "^3.3.2",
        "postcss": "^8.5.6",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "recharts": "^3.6.0",
        "socket.io-client": "^4.8.3",
        "tailwindcss": "^4.1.18",
        "three": "^0.182.0",
        "vite": "^7.3.0",
        "zod": "^4.2.1"
    }
}